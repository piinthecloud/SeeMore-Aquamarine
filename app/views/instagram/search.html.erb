<h2>Search Results</h2>

<% @api.each do |a|%>
<div class= "git_result">
  <%= image_tag "#{a["profile_picture"]}" %>
  <%= a["username"] %>
  <%= form_for Feed.new do |f|  %>
    <%= f.hidden_field :uid, value: a["id"] %>
    <%= f.hidden_field :handle, value: a["username"] %>
    <%= f.hidden_field :social_media, value: "instagram" %>
    <%= f.submit "Subscribe" %>
  <% end %>


  <!-- In the hidden field I am passing though an the id to the controller when it does the post request, so that makes it aviable there -->
  <%#  %>
</div>

<% end %>
